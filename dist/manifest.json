{
    "name": "auto-download-image",
    "version": "1.0.0",
    "manifest_version": 2,

    "description": "可以自动下载图片的插件，支持自动翻页。",
    "permissions": [
        "downloads",
        "tabs",
        "http://*/",
        "https://*/"
    ],

    "browser_action": {
        "default_popup": "popup.html"
    }

}